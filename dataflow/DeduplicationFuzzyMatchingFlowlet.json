		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "adlsgen2employee",
						"type": "DatasetReference"
					},
					"name": "source"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "adlsgen2exployeedupes",
